{
	"compilerOptions" : {
		"allowJs" : true ,
		"allowSyntheticDefaultImports" : true ,
		"allowUnusedLabels" : true ,
		"baseUrl" : "." ,
		"declaration" : true ,
		"declarationDir" : "src/common/dts" ,
		"declarationMap" : true ,
		"esModuleInterop" : true ,
		"experimentalDecorators" : true ,
		"emitDeclarationOnly" : true ,
		"jsx" : "react-jsx" ,
		"resolveJsonModule" : true ,
		"strictFunctionTypes" : false ,
		"strict" : false ,
		"strictBindCallApply" : false ,
		"strictNullChecks" : false ,
		"allowUnreachableCode" : true ,
		"lib" : [
			"ES5" ,
			"ES6" ,
			"ES2015" ,
			"ES7" ,
			"ES2016" ,
			"ES2017" ,
			"ES2018" ,
			"ESNext" ,
			"DOM.Iterable" ,
			"WebWorker" ,
			"ScriptHost" ,
			"ES2015.Core" ,
			"ES2015.Collection" ,
			"ES2015.Generator" ,
			"ES2015.Iterable" ,
			"ES2015.Promise" ,
			"ES2015.Proxy" ,
			"ES2015.Reflect" ,
			"ES2015.Symbol" ,
			"ES2015.Symbol.WellKnown" ,
			"ES2016.Array.Include" ,
			"ES2017.object" ,
			"ES2017.Intl" ,
			"ES2017.SharedMemory" ,
			"ES2017.String" ,
			"ES2017.TypedArrays" ,
			"ES2018.Intl" ,
			"ES2018.Promise" ,
			"ES2018.RegExp" ,
			"ESNext.AsyncIterable" ,
			"ESNext.Array" ,
			"ESNext.Intl" ,
			"ESNext.Symbol" ,
			"react" ,
			"DOM"
		] ,
		"module" : "ESNext" ,
		"moduleResolution" : "node" ,
		"noEmit" : true ,
		"noImplicitAny" : false ,
		"noImplicitReturns" : false ,
		"noImplicitUseStrict" : false ,
		"noUnusedLocals" : false ,
		"noUnusedParameters" : false ,
		"paths" : {
			"@@RootPath" : [
				"/"
			] ,
			"@@common/*" : [
				"src/common/*"
			] ,
			"*/*.ts" : [
				"*"
			] ,
			"@@common/*" : [
				"src/common/*"
			] ,
			"@@utils" : [
				"src/utils/index.tsx"
			] ,
			"@@utils/*" : [
				"src/utils/*"
			] ,
			"@@components" : [
				"src/utils/components/index.ts"
			] ,
			"@@components/*" : [
				"src/utils/components/*"
			] ,
			"@@requester" : [
				"src/common/requester/index"
			] ,
			"@@requester/*" : [
				"src/common/requester/*"
			] ,
			"@@requests" : [
				"src/requests/index"
			] ,
			"@@requests/*" : [
				"src/requests/*"
			] ,
			"@@Public/*" : [
				"Public/*"
			] ,
			"@@pages/*" : [
				"src/pages/*"
			] ,
			"@@assets/*" : [
				"src/assets/*"
			] ,
			"@@lang" : [
				"src/lang/index"
			] ,
			"@@lang/*" : [
				"src/lang/*"
			] ,
			"@@reaxels" : [
				"src/reaxels/index"
			] ,
			"@@reaxels/*" : [
				"src/reaxels/*"
			] ,
			"@@mobxState" : [
				"src/common/MobxState.ts"
			]
			
		} ,
		"preserveConstEnums" : true ,
		"removeComments" : true ,
		"sourceMap" : true ,
		"target" : "ES2020" ,
		"allowSyntheticDefaultImports" : true
	} ,
	"include" : [
		"src" ,
		"src/common" ,
		"src/common/dts"
	] ,
	"exclude" : [
		"node_modules/**" ,
		"dist" ,
		".git" ,
		".idea" ,
		".svn" ,
		"tsconfig.json"
	]
}
